    "debug": "meteor debug --settings settings.json",
    "test": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "chimp-headless": "./node_modules/.bin/chimpy .config/chimp.js --ddp=http://localhost:5000 --path=tests",
    "chimp": "CUCUMBER_BROWSERS=2 ./node_modules/.bin/chimpy --ddp=http://localhost:5000 --path=tests",
    "chimp:watch": "CUCUMBER_BROWSERS=2 ./node_modules/.bin/chimpy .config/chimp.js --ddp=http://localhost:5000 --watch --path=tests",
