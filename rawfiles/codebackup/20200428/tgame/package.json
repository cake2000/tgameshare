  "uglifyjs2": {
    "deadCodes": [
      "_meteor.Meteor.isServer",
      "Meteor.isServer"
    ],
    "fileRemoval": [
      "packages/ddp-server.js",
      "packages/shell-server.js",
      "packages/ssrwpo_uglifyjs2.js"
    ],
    "packageDebug": false,
    "options": {
      "fromString": true,
      "compress": {
        "properties": false,
        "dead_code": true,
        "drop_debugger": true,
        "conditionals": true,
        "comparisons": true,
        "evaluate": true,
        "booleans": true,
        "loops": true,
        "unused": false,
        "hoist_funs": true,
        "if_return": true,
        "join_vars": true,
        "cascade": true,
        "collapse_vars": true,
        "negate_iife": true,
        "pure_getters": true,
        "drop_console": false,
        "keep_fargs": false,
        "keep_fnames": false,
        "passes": 2,
        "global_defs": {
          "UGLYFYJS_DEAD": false
        }
      }
    }
